  <div class="beige-wrapper">
  
            <div class="heading"><h2>労働局情報一覧</h2>
            <p>Prefecture List    <%= link_to '(New regstration)', '/prefectures/new' %></p></div>
    
    <table>
     <tbody>
      <tr>
               <th>各労働局情報</th>
			   <th>最低賃金</th>
			   <th>該当助成金</th>
			   <th>提出方法</th>
			   <th>電話番号</th>
	           <th>郵便番号</th>
	           <th>住所</th>
	    	   <th>注意点</th>
               <th>編集/削除</th>
　  </tr>
      <tr>
      <% @prefectures.each do |prefecture|%>
        <td><%= prefecture.labor%></td>
        <td><%= prefecture.minimum_wage%></td>
        <td><%= prefecture.product_type%></td>
        <td><%= prefecture.method%></td>
        <td><%= prefecture.tel%></td>
        <td><%= prefecture.post%></td>
        <td><%= prefecture.address%></td>
        <td><%= prefecture.point%></td>
        <td><%= link_to '編集', edit_prefecture_path(prefecture), class: 'command'%>
			            ｜ <%= link_to '削除', 
			            	prefecture_path(prefecture), 
			            	method: :delete, 
			            	class: 'command',
			            	data: { confirm: '本当に削除しますか？'} %></td>

      </tr>
      <% end %>
      </tbody>
    </table> 



 </div>
